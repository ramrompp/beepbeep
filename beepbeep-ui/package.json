{
  "name": "beepbeep",
  "version": "1.0.0",
  "description": "Frontend for Mepa-Store Market Ad Service",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "bower": "^1.4.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.12.32",
    "http-server": "^0.6.1",
    "karma-chrome-launcher": "^0.1.12",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "protractor": "^1.1.1"
  },
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
